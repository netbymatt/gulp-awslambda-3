	"version": "0.2.0",
	"configurations": [
		{
			"name": "Gulp test",
			"cwd": "${workspaceFolder}/test",
			"program": "${workspaceFolder}/test/test.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/**/node_modules/**",
				"${workspaceFolder}/node_modules/**",
				"${workspaceFolder}\\**\\node_modules\\**\\*.js",
				"${workspaceFolder}\\node_modules\\**\\*.js",
			],
			"type": "pwa-node",
			"outputCapture": "std",
		},
	]
}